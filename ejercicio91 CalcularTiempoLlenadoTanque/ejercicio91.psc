Algoritmo CalcularTiempoLlenadoTanque
    Leer L
    CapacidadEnLitros <- 50 * 1000
    TiempoEnMinutos <- CapacidadEnLitros / L
    TiempoEnHoras <- TiempoEnMinutos / 60
    Escribir "El tiempo en minutos es: ", TiempoEnMinutos
    Escribir "El tiempo en horas es: ", TiempoEnHoras
FinAlgoritmo
