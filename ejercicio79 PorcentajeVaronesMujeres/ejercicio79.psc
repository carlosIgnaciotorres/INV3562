Algoritmo PorcentajeVaronesMujeres
    Escribir "Ingresa la cantidad de varones: "
    Leer varones
    Escribir "Ingresa la cantidad de mujeres: "
    Leer mujeres
    total <- varones + mujeres
    porcentajeVarones <- (varones / total) * 100
    porcentajeMujeres <- (mujeres / total) * 100
    Escribir "Porcentaje de varones: ", porcentajeVarones, "%"
    Escribir "Porcentaje de mujeres: ", porcentajeMujeres, "%"
FinAlgoritmo

